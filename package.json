{
  "name": "fullstack",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development --host",
    "dev:production": "vite --mode production --host",
    "build": "rm -rf ./build && yarn build:client && yarn build:server",
    "build:client": "vite build --mode client && node ./scripts/rmtml.mjs",
    "build:server": "vite build --mode server",
    "start:test": "cross-env mode=development node ./build/app.js",
    "start:prod": "cross-env mode=production node ./build/app.js",
    "commit": "git add . && git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "czConfig": "./commitlint.config.js"
    }
  },
  "_moduleAliases": {
    "client": "client",
    "server": "server",
    "utils": "utils",
    "types": "types"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.0",
    "@ant-design/pro-layout": "^7.22.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@hono/node-server": "^1.14.2",
    "@ljaq/editor": "^0.1.0",
    "@react-spring/web": "^9.7.5",
    "@react-three/drei": "^10.1.2",
    "@react-three/fiber": "8",
    "@tanstack/react-query": "^5.67.2",
    "@tanstack/react-query-devtools": "^5.67.2",
    "@tanstack/router-plugin": "^1.106.0",
    "antd": "^5.24.1",
    "antd-img-crop": "^4.24.0",
    "antd-style": "^3.7.1",
    "boring-avatars": "^1.11.2",
    "canvas-datagrid": "^0.4.6",
    "dayjs": "^1.11.13",
    "docx-preview": "^0.1.20",
    "echarts": "^6.0.0",
    "echarts-for-react": "^3.0.2",
    "form-render": "^2.5.3",
    "hono": "^4.7.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.5.3",
    "react-router-dom": "^7.5.3",
    "react-use": "^17.6.0",
    "three": "^0.177.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "vanta": "^0.5.24",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@hono/vite-build": "^1.6.1",
    "@hono/vite-dev-server": "^0.19.0",
    "@swc/core": "^1.11.24",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cz-git": "^1.11.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "less": "^4.2.2",
    "module-alias": "^2.2.3",
    "mwtsc": "^1.15.1",
    "prettier": "^3.4.2",
    "ts-node-dev": "^2.0.0",
