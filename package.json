{
  "name": "fullstack",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "NODE_ENV=dev tsnd --respawn --project ./tsconfig.server.json app.ts",
    "build": "rm -rf ./dist && yarn generate && vite build && tsc --project tsconfig.server.json",
    "start:prod": "NODE_ENV=prod node ./dist/app.js",
    "start:test": "NODE_ENV=test node ./dist/app.js",
    "commit": "git add . && git-cz",
    "generate": "ts-node --project ./tsconfig.server.json ./scripts/generateServerApi.ts -with-bind"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "czConfig": "./commitlint.config.js"
    }
  },
  "_moduleAliases": {
    "client": "client",
    "server": "server",
    "utils": "utils",
    "types": "types"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.0",
    "@ant-design/pro-layout": "^7.22.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@ljaq/editor": "^0.0.4",
    "@tanstack/react-router": "^1.106.0",
    "@tanstack/router-devtools": "^1.106.0",
    "@tanstack/router-plugin": "^1.106.0",
    "antd": "^5.23.2",
    "antd-img-crop": "^4.24.0",
    "canvas-datagrid": "^0.4.6",
    "daruk": "^2.4.13",
    "docx-preview": "^0.1.20",
    "echarts": "^5.6.0",
    "http-proxy-middleware": "^2.0.6",
    "koa-static": "^5.0.0",
    "koa2-connect-history-api-fallback": "^0.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.1.3",
    "react-router-dom": "^7.1.3",
    "react-use": "^17.6.0",
    "ts-node": "^10.9.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cz-git": "^1.11.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "koa-connect": "^2.1.0",
    "less": "^4.2.2",
    "module-alias": "^2.2.3",
    "mwtsc": "^1.15.1",
    "prettier": "^3.4.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-pages": "^0.32.4"
  }
}
